{
	"compilerOptions": {
		"strict": true,
		"target": "ES2016",
		"module": "ESNext",
		"moduleResolution": "node",
		"rootDir": "./s",
		"outDir": "./x",
		"declaration": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"lib": ["ESNext", "DOM"]
	},
	"include": [
		"s/**/*.ts",
		"s/**/*.js"
	],
	"exclude": [
		"s/**/_*"
	]
}
  